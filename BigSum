#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int testNum;
    long long ld;
    long long input;
    ld = 0;
    cin >> testNum;
    for (int i=0; i<testNum; ++i){
        cin >> input;
        ld += input;
    }
    cout << ld;
    return 0;
}
